import java.util.ArrayList; 
public class It
{

    private int x = 78;
    public It24( ) {
        x = 99;
    }  

    public It24( int y ) {
        x = y; }

    public String toString(){
        return "" + x;
    }

    public static void main(){

        ArrayList<It> itList = new ArrayList<It>();
        itList.add(new It24());
        itList.add(new It24(8));
        System.out.println(itList);

    }
}
